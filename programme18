//program to calculate  the factorial
#include <iostream>
using namespace std;
int main(){
 int i,fact=1,n;
 cout<<"Factorial of any integer "<<endl;
 cout<<"enter the number"<<endl;
 cin>>n;
 for (i = 1; i <= n; i++)
 {
     fact = fact*i;
 }
 cout<<"the factorial of "<<n<<" is "<<fact;
return 0;
}
