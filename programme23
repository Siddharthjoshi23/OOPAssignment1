// pattern
#include <iostream> 
using namespace std;
int main()
{
 int i, j;
 for(i=5; i>=1; i--)
 {
  for(j=i; j>=1; j--)
   cout << j;
  cout << "\n"; 
 }
 return 0;
}
Program 24 : A program to find sum of the first 100 natural numbers
//sum of 100 natural numbers
#include<iostream>
using namespace std;
int main()
{
int i,n=100,sum=0;
for(i=1;i<=n;++i)
{
sum+=i;
}
cout<<"Sum of first 100 natural numbers is "<<sum;
return 0;
}
