//Write a program for convert temperature from celsius to Fahrenheit and Fahrenheit to Celsius
// program for convert temperature from celsius to Fahrenheit
#include<iostream>
using namespace std;
int main()
{
    float f,c;
    cout << "Enter the temperature in Celsius : ";
    cin >> c;
    f = (c * 9.0) / 5.0 + 32;
    cout << "The temperature in Fahrenheit : " << f << endl;
    return 0;
}
