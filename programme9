// Write a program for input for two numbers and check whether  it is equal or not
#include<iostream>
using namespace std;
int main()
{
    float a, b;
    cout << "Input the values for Number1 and Number2 :";
    cin >> a >> b;
    if (a == b)
    {
        cout << "Number1 and Number2 are equal";
    }
    else
    {
        cout << "Number1 and Number2 are not equal";
    }
}
